#include <stdio.h>

int main() {
	// Write your code here
	int inputNumber;
    scanf("%d", &inputNumber);

    int temp = inputNumber;
    int numberOfDigits = 0;

    while (temp != 0) {
        temp /= 10;
        numberOfDigits++;
    }

    temp = inputNumber;
    int armstrongValue = 0;

    while (temp != 0) {
        int lastDigit = temp % 10;
        armstrongValue += pow(lastDigit, numberOfDigits);
        temp /= 10;
    }

    if (armstrongValue == inputNumber) {
        printf("true");
    } else {
        printf("false");
    }

    return 0;
}
